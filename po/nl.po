msgstr " aantal zinnen: %d "

#: src/eBook-speaker.c:985 src/list_dir.c:182
msgid "These commands are available in this version:"
msgstr "Deze commando's zijn beschikbaar in deze versie:"

#: src/eBook-speaker.c:989 src/list_dir.c:186
msgid "cursor down,2   - move cursor to the next item"
msgstr "cursor omlaag,2 - verplaats de cursor naar het volgende item"

#: src/eBook-speaker.c:991 src/list_dir.c:188
msgid "cursor up,8     - move cursor to the previous item"
msgstr "cursor omhoog,8 - verplaats de cursor naar het vorige item"

#: src/eBook-speaker.c:993
msgid "cursor right,6  - skip to next phrase"
msgstr "cursor rechts,6 - spring naar de volgende frase"

#: src/eBook-speaker.c:995
msgid "cursor left,4   - skip to previous phrase"
msgstr "cursor links,4  - spring naar de vorige frase"

#: src/eBook-speaker.c:997 src/list_dir.c:194
msgid "page-down,3     - view next page"
msgstr "page-down,3     - toon de volgende pagina"

#: src/eBook-speaker.c:999 src/list_dir.c:196
msgid "page-up,9       - view previous page"
msgstr "page-up,9       - toon de vorige pagina"

#: src/eBook-speaker.c:1001
msgid "enter           - start playing"
msgstr "enter           - start afspelen"

#: src/eBook-speaker.c:1003
msgid "space,0         - pause/resume playing"
msgstr "spatie,0        - pauze/vervolg het afspelen"

#: src/eBook-speaker.c:1005
msgid "home,*          - play on normal speed"
msgstr "home,*          - speel af op normale snelheid"

#: src/eBook-speaker.c:1007 src/eBook-speaker.c:1049
msgid "Press any key for next page..."
msgstr "Druk op een toets voor de volgende pagina..."

#: src/eBook-speaker.c:1014 src/list_dir.c:200
msgid "/               - search for a label"
msgstr "/               - zoek naar een label"

#: src/eBook-speaker.c:1016
msgid "A               - store current item to disk in ASCII-format"
msgstr "A               - sla huidige item op schijf op in ASCII-formaat"

#: src/eBook-speaker.c:1018
msgid "b               - consider the end of an input line as a phrase-break"
msgstr ""
"b               - beschouw het einde van een invoer-regel als een zins-einde"

#: src/eBook-speaker.c:1020
msgid "B               - move cursor to the last item"
msgstr "B               - verplaats de cursor naar het laatste item"

#: src/eBook-speaker.c:1022
msgid "d               - store current item to disk in WAV-format"
msgstr "d               - sla huidige item op schijf op in WAV-formaat"

#: src/eBook-speaker.c:1024
msgid "D,-             - decrease playing speed"
msgstr "D,-             - verlaag afspeel snelheid"

#: src/eBook-speaker.c:1026
msgid ""
"f               - find the currently playing item and place the cursor there"
msgstr ""
"f               - vindt het huidige spelende item en plaats de cursor daar"

#: src/eBook-speaker.c:1028
msgid "g               - go to phrase in current item"
msgstr "g               - ga naar zinsdeel in dit item"

#: src/eBook-speaker.c:1031
msgid "G               - go to page number"
msgstr "G               - ga naar pagina nummer"

#: src/eBook-speaker.c:1033 src/list_dir.c:204
msgid "h,?             - give this help"
msgstr "h,?             - toon deze help-pagina"

#: src/eBook-speaker.c:1035
msgid "j,5             - just play current item"
msgstr "j,5             - speel alleen dit item af"

#: src/eBook-speaker.c:1037
msgid "l               - switch to next level"
msgstr "l               - schakel naar het volgende niveau"

#: src/eBook-speaker.c:1039
msgid "L               - switch to previous level"
msgstr "L               - schakel naar het vorige niveau"

#: src/eBook-speaker.c:1041 src/list_dir.c:208
msgid "n               - search forwards"
msgstr "n               - zoek voorwaarts"

#: src/eBook-speaker.c:1043 src/list_dir.c:210
msgid "N               - search backwards"
msgstr "N               - zoek achteruit"

#: src/eBook-speaker.c:1045
msgid "o               - select next output sound device"
msgstr "o               - selecteer de volgende geluidskaart"

#: src/eBook-speaker.c:1047
msgid "p               - place a bookmark"
msgstr "p               - plaats een boekenlegger"

#: src/eBook-speaker.c:1056
msgid "q               - quit eBook-speaker and place a bookmark"
msgstr "q               - stop eBook-speaker en plaats een boekenlegger"

#: src/eBook-speaker.c:1059
msgid "r               - rotate scanned document"
msgstr "r               - draai het gescande dokument om"

#: src/eBook-speaker.c:1061
msgid "s               - stop playing"
msgstr "s               - stop het afspelen"

#: src/eBook-speaker.c:1063
msgid "t               - select next TTS"
msgstr "t               - selecteer een andere TTS"

#: src/eBook-speaker.c:1065
msgid "T               - move cursor to the first item"
msgstr "T               - verplaats de cursor naar het eerste item"

#: src/eBook-speaker.c:1067
msgid "U,+             - increase playing speed"
msgstr "U,+             - verhoog afspeel snelheid"

#: src/eBook-speaker.c:1069
msgid "v,1             - decrease playback volume"
msgstr "v,1             - verlaag het volume"

#: src/eBook-speaker.c:1071
msgid "V,7             - increase playback volume (beware of Clipping)"
msgstr "V,7             - verhoog het volume (pas op voor oversturing)"

#: src/eBook-speaker.c:1073
msgid "x               - go to the file-manager"
msgstr "x               - ga naar bestands-beheer"

#: src/eBook-speaker.c:1075 src/list_dir.c:216
msgid "Press any key to leave help..."
msgstr "Druk op een toets om dit help-scherm te verlaten..."

#: src/eBook-speaker.c:1322 src/list_dir.c:118
msgid "What do you search? "
msgstr "Wat zoekt u? "

#: src/eBook-speaker.c:1407
msgid "Consider the end of an input line as a phrase-break (y/n) "
msgstr "Beschouw het einde van een invoer-regel als een zins-einde (y/n) "

#: src/eBook-speaker.c:1444
msgid "Select a soundcard:"
msgstr "Selecteer een geluidskaat"

#: src/eBook-speaker.c:1446
msgid "Cannot read /proc/asound/cards"
msgstr "Kan /proc/asound/cards niet lezen"

#: src/eBook-speaker.c:1663
#, c-format
msgid "Language code \"%s\" is not a valid tesseract code.\n"
msgstr "Taal code \"%s\" is geen geldige code.\n"

#: src/eBook-speaker.c:1665
msgid "See the tesseract manual for valid codes."
msgstr "Zie de tesseract manual voor geldige codes."

#: src/eBook-speaker.c:1667
#, c-format
msgid ""
"Be sure the package \"tesseract-ocr-%s\" is installed onto your system.\n"
msgstr ""
"Controleer of het pakket \"tesseract-ocr-%s\" op uw systeem geinstalleerd "
"is\n"

#: src/eBook-speaker.c:1675
#, c-format
msgid "Language code \"%s\" is not a valid cuneiform code.\n"
msgstr "Taal code \"%s\" is geen geldige cuneiform code.\n"

#: src/eBook-speaker.c:1677
msgid "See the cuneiform manual for valid codes."
msgstr "Zie de cuneiform manual voor geldige codes."

#: src/eBook-speaker.c:1697
msgid "Go to page number: "
msgstr "Ga naar pagina nummer: "

#: src/eBook-speaker.c:1863
msgid "Go to phrase in current item: "
msgstr "Ga naar zinsdeel in huidige item: "

#: src/eBook-speaker.c:1989 src/eBook-speaker.c:2715 src/eBook-speaker.c:2978
msgid "Please wait..."
msgstr "Geduld a.u.b...."

#: src/eBook-speaker.c:2002 src/eBook-speaker.c:2255 src/eBook-speaker.c:2272
#: src/eBook-speaker.c:2293 src/eBook-speaker.c:2323 src/eBook-speaker.c:2536
#: src/eBook-speaker.c:2755 src/eBook-speaker.c:2797 src/eBook-speaker.c:3054
msgid "eBook-speaker cannot handle this file."
