msgstr "%s - Kies een invoer-bestand"

#: src/list_dir.c:191
msgid "cursor right,6  - open this directory or file"
msgstr "cursor rechts,6 - open dit bestand of  deze map"

#: src/list_dir.c:193
msgid "cursor left,4   - open previous directory"
msgstr "cursor links,4  - open vorige map"

#: src/list_dir.c:199
msgid "enter,6         - open this directory or file"
msgstr "enter,6         - open dit bestand of deze map"

#: src/list_dir.c:203
msgid "end,B           - move cursor to the last item"
msgstr "end,B           - verplaats de cursor naar het laatste item"

#: src/list_dir.c:207
msgid "H,0             - display \"hidden\" files on/off"
msgstr "H,0             - toon \"verborgen\" bestanden aan/uit"

#: src/list_dir.c:213
msgid "q               - quit eBook-speaker"
msgstr "q               - stop eBook-speaker"

#: src/list_dir.c:215
