#: src/list_dir.c:181
msgid "cursor left,4   - open previous directory"
msgstr "cursor links,4  - open vorige map"

#: src/list_dir.c:187
msgid "enter,6         - open this directory or file"
msgstr "enter,6         - open dit bestand of deze map"

#: src/list_dir.c:191
