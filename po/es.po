#: src/eBook-speaker.c:890 src/list_dir.c:140
msgid ""
"\n"
"These commands are available in this version:\n"
msgstr ""
"\n"
"Estas orden estan disponibles en esta versión:\n"

#: src/eBook-speaker.c:893 src/list_dir.c:143
msgid "cursor down     - move cursor to the next item\n"
msgstr "cursor abajo     - desplazar el cursor para el tema próximo\n"

#: src/eBook-speaker.c:894 src/list_dir.c:144
msgid "cursor up       - move cursor to the previous item\n"
msgstr "cursor arriba    - desplazar el cursor para el tema anterior\n"

#: src/eBook-speaker.c:895
msgid "cursor right    - skip to next phrase\n"
msgstr "cursor derecha   - saltar para la frase próxima\n"

#: src/eBook-speaker.c:896
msgid "cursor left     - skip to previous phrase\n"
msgstr "cursor izquierdo - saltar para la frase anterior\n"

#: src/eBook-speaker.c:897 src/list_dir.c:147
msgid "page-down       - view next page\n"
msgstr "página-abaja     - ir a ver la página próxima\n"

#: src/eBook-speaker.c:898 src/list_dir.c:148
msgid "page-up         - view previous page\n"
msgstr "página-arriba    - ir a ver la página anterior\n"

#: src/eBook-speaker.c:899
msgid "enter           - start playing\n"
msgstr "entrar           - comenzar escuchar\n"

#: src/eBook-speaker.c:900
msgid "space           - pause/resume playing\n"
msgstr "espacio          - pausa/reanudar escuchar\n"

#: src/eBook-speaker.c:901
msgid "home            - play on normal speed\n"
msgstr "inicio           - escuchar a velocidad normal\n"

#: src/eBook-speaker.c:903
msgid "Press any key for next page..."
msgstr "Hacer clic cada tecla para la página proxima..."

#: src/eBook-speaker.c:908
msgid ""
"\n"
"/               - search for a label\n"
msgstr ""
"\n"
"/                - buscar por una etiqueta\n"

#: src/eBook-speaker.c:909 src/list_dir.c:152
msgid "B               - move cursor to the last item\n"
msgstr "B                - desplazar el cursor hasta el último tema\n"

#: src/eBook-speaker.c:910
msgid "d               - store current item to disk in WAV-format\n"
msgstr "d                - almacenar el tema actual al disco en WAV-format\n"

#: src/eBook-speaker.c:912
msgid "D               - decrease playing speed\n"
msgstr "D                - disminuir velocidad de escuchar\n"

#: src/eBook-speaker.c:913
msgid ""
"f               - find the currently playing item and place the cursor "
"there\n"
msgstr ""
"f                - encontrar el item escuchando y poner el cursor allí\n"

#: src/eBook-speaker.c:914
msgid "g               - go to phrase in current item\n"
msgstr "g                - ir a la frase en actual tema\n"

#: src/eBook-speaker.c:915 src/list_dir.c:153
msgid "h or ?          - give this help\n"
msgstr "h o ?            - da esta ayuda\n"

#: src/eBook-speaker.c:916
msgid "j               - just play current item\n"
msgstr "j                - solo escuchar el tema actual\n"

#: src/eBook-speaker.c:917
msgid "l               - switch to next level\n"
msgstr "l                - cambiar al nivel proximo\n"

#: src/eBook-speaker.c:918
msgid "L               - switch to previous level\n"
msgstr "L                - cambiar al nivel anterior\n"

#: src/eBook-speaker.c:919 src/list_dir.c:155
msgid "n               - search forwards\n"
msgstr "n                - buscar adelante\n"

#: src/eBook-speaker.c:920 src/list_dir.c:156
msgid "N               - search backwards\n"
msgstr "N                - buscar atrás\n"

#: src/eBook-speaker.c:921
msgid "o               - select next output sound device\n"
msgstr ""
"o                - seleccionar la proxima salida de dispositivo sonido\n"

#: src/eBook-speaker.c:922
msgid "p               - place a bookmark\n"
msgstr "p                - poner un marcapáginas\n"

#: src/eBook-speaker.c:923
msgid "q               - quit eBook-speaker and place a bookmark\n"
msgstr "q                - dejar eBook-speaker y poner un marcapáginas\n"

#: src/eBook-speaker.c:924
msgid "s               - stop playing\n"
msgstr "s                - parar escuchar\n"

#: src/eBook-speaker.c:925
msgid "t               - select next TTS\n"
msgstr "t                - seleccionar próximo TTS\n"

#: src/eBook-speaker.c:926 src/list_dir.c:158
msgid "T               - move cursor to the first item\n"
msgstr "T                - desplazar el cursor para el primero tema\n"

#: src/eBook-speaker.c:927
msgid "U               - increase playing speed\n"
msgstr "U                - acelerar velocidad de escuchar\n"

#: src/eBook-speaker.c:928 src/list_dir.c:159
msgid ""
"\n"
"Press any key to leave help..."
msgstr ""
"\n"
"Hacer clic cada tecla para salir ayudar..."

