#, c-format
msgid "Cannot read %s"
msgstr "Não consegue ler %s"

#: src/daisy3.c:497
#, c-format
msgid "Cannot read type: %d"
msgstr "Não consegue ler tipo: %d"

#: src/eBook-speaker.c:158
msgid "Select a Text-To-Speech application"
msgstr "Selecione um texto para aplicação Text-To-Speach"

#: src/eBook-speaker.c:170
msgid "Provide a new TTS."
msgstr "Providencie uma nova TTS."

#: src/eBook-speaker.c:172
msgid "Be sure that the new TTS reads its information from the file"
msgstr "A segure que esse novo TTS leia a informação do file"

#: src/eBook-speaker.c:174
msgid "eBook-speaker.txt and that it writes to the file eBook-speaker.wav."
msgstr "eBook-speaker.txt e que escreva no documento eBook-speaker.wav."

#: src/eBook-speaker.c:176
msgid "Press DEL to delete a TTS"
msgstr "Pressione DEL para deletar o TTS"

#: src/eBook-speaker.c:563
#, c-format
msgid ""
"Be sure that the used TTS is installed:\n"
"\n"
"   \"%s\"\n"
msgstr ""

#: src/eBook-speaker.c:712 src/list_dir.c:69 src/list_dir.c:263
msgid "'h' for help "
msgstr "'h' para ajuda "

#: src/eBook-speaker.c:717
#, c-format
msgid " %d pages "
msgstr " %d páginas "

#: src/eBook-speaker.c:719
#, c-format
msgid " level: %d of %d "
msgstr " nível: %d de %d "

#: src/eBook-speaker.c:721
#, c-format
msgid " total phrases: %d "
msgstr " Total de frases: %d "

#: src/eBook-speaker.c:949 src/list_dir.c:180
msgid "These commands are available in this version:"
msgstr "Esses comandos estão disponíveis nessa versão:"

#: src/eBook-speaker.c:953 src/list_dir.c:184
msgid "cursor down,2   - move cursor to the next item"
msgstr "cursor para baixo,2    - move o cursor para o próximo item"

#: src/eBook-speaker.c:955 src/list_dir.c:186
msgid "cursor up,8     - move cursor to the previous item"
msgstr "cursor para cima,8     - move o cursor para o item anterior"

#: src/eBook-speaker.c:957
msgid "cursor right,6  - skip to next phrase"
msgstr "cursor para direita,6  - pula para a proxima frase"

#: src/eBook-speaker.c:959
msgid "cursor left,4   - skip to previous phrase"
msgstr "cursor para esquerda,4 - pula para a frase anterior"

#: src/eBook-speaker.c:961 src/list_dir.c:192
msgid "page-down,3     - view next page"
msgstr "page-down,3            - ver página seguinte"

#: src/eBook-speaker.c:963 src/list_dir.c:194
msgid "page-up,9       - view previous page"
msgstr "page-up,9              - ver página anterior"

#: src/eBook-speaker.c:965
msgid "enter           - start playing"
msgstr "enter                  - comece a reprodução"

#: src/eBook-speaker.c:967
msgid "space,0         - pause/resume playing"
msgstr "espaço,0               - pausa/ resumir reprodução"

#: src/eBook-speaker.c:969
msgid "home,*          - play on normal speed"
msgstr "home,*                 - reproduzir em velocidade normal"

#: src/eBook-speaker.c:971 src/eBook-speaker.c:1013
msgid "Press any key for next page..."
msgstr "Pressione qualquer tecla para a próxima página..."

#: src/eBook-speaker.c:978 src/list_dir.c:198
msgid "/               - search for a label"
msgstr "/                      - procure por uma etiqueta"

#: src/eBook-speaker.c:980
msgid "A               - store current item to disk in ASCII-format"
msgstr ""
"A                      - armazena o item atual no disco no formato ASCII"

#: src/eBook-speaker.c:982
msgid "b               - consider the end of an input line as a phrase-break"
msgstr ""

#: src/eBook-speaker.c:984
msgid "B               - move cursor to the last item"
msgstr "B                      - move o cursor para o último ítem"

#: src/eBook-speaker.c:986
msgid "d               - store current item to disk in WAV-format"
msgstr "d                      - armazena o item atual no disco em formato WAV"

#: src/eBook-speaker.c:988
msgid "D,-             - decrease playing speed"
msgstr "D,-                    - diminui a velocidade de reprodução"

#: src/eBook-speaker.c:990
msgid ""
"f               - find the currently playing item and place the cursor there"
msgstr ""
"f                      - acha o item atual de reprodução e coloca o cursor lá"

#: src/eBook-speaker.c:992
msgid "g               - go to phrase in current item"
msgstr "g                      - vai para a frase no item atual"

#: src/eBook-speaker.c:995
msgid "G               - go to page number"
msgstr "G                      - vai para o número de pagina (se tiver algum)"

#: src/eBook-speaker.c:997 src/list_dir.c:202
msgid "h,?             - give this help"
msgstr "h,?                    - dê essa ajuda"

#: src/eBook-speaker.c:999
msgid "j,5             - just play current item"
msgstr "j,5                    - só reproduza o item atual"

#: src/eBook-speaker.c:1001
msgid "l               - switch to next level"
msgstr "l                      - mude para o proximo nível"

#: src/eBook-speaker.c:1003
msgid "L               - switch to previous level"
msgstr "L                      - mude para o nível anterior"

#: src/eBook-speaker.c:1005 src/list_dir.c:206
msgid "n               - search forwards"
msgstr "n                      - procure a frente"

#: src/eBook-speaker.c:1007 src/list_dir.c:208
msgid "N               - search backwards"
msgstr "N                      - procure para trás"

#: src/eBook-speaker.c:1009
msgid "o               - select next output sound device"
msgstr ""
"o                      - selecione o próximo dispositivo de saída de som"

#: src/eBook-speaker.c:1011
msgid "p               - place a bookmark"
msgstr "p                      - coloque o marcador"

#: src/eBook-speaker.c:1020
msgid "q               - quit eBook-speaker and place a bookmark"
msgstr "q                      - saia do eBook-speaker e coloque o marcador"

#: src/eBook-speaker.c:1023
msgid "r               - rotate scanned document"
msgstr "r                      - gire documento scaneado"

#: src/eBook-speaker.c:1025
msgid "s               - stop playing"
msgstr "s                      - pare a reprodução"

#: src/eBook-speaker.c:1027
msgid "t               - select next TTS"
msgstr "t                      - selecione o próximo TTS"

#: src/eBook-speaker.c:1029
msgid "T               - move cursor to the first item"
msgstr "T                      - mova o cursor para o primeiro item"

#: src/eBook-speaker.c:1031
msgid "U,+             - increase playing speed"
msgstr "U,+                    - aumente a velocidade de reprodução"

#: src/eBook-speaker.c:1033
msgid "v,1             - decrease playback volume"
msgstr ""

#: src/eBook-speaker.c:1035
msgid "V,7             - increase playback volume (beware of Clipping)"
msgstr ""

#: src/eBook-speaker.c:1037
msgid "x               - go to the file-manager"
msgstr ""

#: src/eBook-speaker.c:1039 src/list_dir.c:214
msgid "Press any key to leave help..."
msgstr "Pressione qualquer tecla para sair da ajuda..."

#: src/eBook-speaker.c:1286 src/list_dir.c:116
msgid "What do you search? "
msgstr "O que você procura? "

#: src/eBook-speaker.c:1371
msgid "Consider the end of an input line as a phrase-break (y/n) "
msgstr ""

#: src/eBook-speaker.c:1408
msgid "Select a soundcard:"
msgstr "Selecione uma placa de som:"

#: src/eBook-speaker.c:1410
msgid "Cannot read /proc/asound/cards"
msgstr "Não consegue ler /proc/asound/cards"

#: src/eBook-speaker.c:1510
msgid "Go to page number: "
msgstr "Vá para a página número: "

#: src/eBook-speaker.c:1647
#, c-format
msgid "Language code \"%s\" is not a valid tesseract code.\n"
msgstr "Linguagem codigo \"%s\"  tesseract codigo não é válido.\n"

#: src/eBook-speaker.c:1649
msgid "See the tesseract manual for valid codes."
msgstr "Olhe o manual tesseract para codigos válidos."

#: src/eBook-speaker.c:1651
#, c-format
msgid ""
"Be sure the package \"tesseract-ocr-%s\" is installed onto your system.\n"
msgstr ""

#: src/eBook-speaker.c:1733
msgid "Go to phrase in current item: "
msgstr "Vá para frase no item atual: "

#: src/eBook-speaker.c:1859 src/eBook-speaker.c:2578 src/eBook-speaker.c:2812
msgid "Please wait..."
msgstr "Por favor espere..."

#: src/eBook-speaker.c:1872 src/eBook-speaker.c:2119 src/eBook-speaker.c:2138
#: src/eBook-speaker.c:2159 src/eBook-speaker.c:2191 src/eBook-speaker.c:2403
#: src/eBook-speaker.c:2616 src/eBook-speaker.c:2665 src/eBook-speaker.c:2868
msgid "eBook-speaker cannot handle this file."
msgstr "eBook-speaker não consegue lidar com esse arquivo."

#: src/eBook-speaker.c:1908 src/eBook-speaker.c:2543
msgid "Press 'h' for help "
msgstr "Pressione 'h' para ajuda "

#: src/eBook-speaker.c:2167
msgid "Unknown format"
msgstr "Formato desconhecido"

#: src/eBook-speaker.c:2374 src/eBook-speaker.c:2809
msgid "eBook-speaker - Version"
msgstr "eBook-speaker - Versão"

#: src/eBook-speaker.c:2378
#, c-format
msgid ""
"Usage: %s [eBook_file | -s] [-o language-code] [-d ALSA_sound_device] [-h] [-"
"i] [-t TTS_command] [-b n | y]"
msgstr ""
"Use: %s [eBook_archivo | -s] [-o codigo_de_linguagem]] [-d "
"ALSA_sistema_de_som] [-h] [-i] [-t TTS_comando] [-b n | y]"

#: src/list_dir.c:65 src/list_dir.c:260
#, c-format
msgid "%s - Choose an input-file"
msgstr "%s – Selecione um arquivo de entrada"

#: src/list_dir.c:188
msgid "cursor right,6  - open this directory or file"
msgstr "cursor para direita,6  - abre esse diretório ou arquivo"

#: src/list_dir.c:190
msgid "cursor left,4   - open previous directory"
msgstr "cursor para esquerda,4 - abre diretório anterior"

#: src/list_dir.c:196
msgid "enter,6         - open this directory or file"
msgstr "enter,6                - abre esse diretório ou arquivo"

#: src/list_dir.c:200
msgid "end,B           - move cursor to the last item"
msgstr "end,B                  - move o cursor para o último ítem"

#: src/list_dir.c:204
msgid "H,0             - display \"hidden\" files on/off"
msgstr "H,0                    - exibe \"esconde\" arquivos ligado/desligado"

#: src/list_dir.c:210
msgid "q               - quit eBook-speaker"
msgstr "q                      - sair eBook-speaker"

#: src/list_dir.c:212
msgid "home,T          - move cursor to the first item"
msgstr "home,T                 - mova o cursor para o primeiro item"

