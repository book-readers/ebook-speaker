#: src/common.c:35 src/common.c:289 src/common.c:503 src/ebook-speaker.c:2085
msgid "Please try to play this book with daisy-player"
msgstr "Molimo pokušajte otvoriti ovu knjigu pomoću daisy-plejera"

#: src/common.c:770
#, c-format
msgid "Cannot read type: %d"
msgstr "Ne mogu pročitati napisano: %d"

#: src/common.c:835
msgid "Go to page number:"
msgstr "Idi na stranu broj:"

#: src/common.c:853 src/ebook-speaker.c:1677 src/ebook-speaker.c:2463
#: src/ebook-speaker.c:2760
msgid "Please wait..."
msgstr "Molimo sačekajte..."

#: src/common.c:996
msgid "Select a soundcard:"
msgstr "Izaberite zvučnu kartu:"

#: src/daisy2.02.c:49 src/daisy2.02.c:130 src/daisy2.02.c:236
#: src/ebook-speaker.c:2351
#, c-format
msgid "Cannot read %s"
msgstr "Ne mogu pročitati %s"

#: src/ebook-speaker.c:301
msgid "Select a Text-To-Speech application"
msgstr "Izaberi Text-To-Speach aplikaciju"

#: src/ebook-speaker.c:318
msgid "Maximum number of TTS's is reached."
msgstr "Maksimalni broj TTS-a je dostignut."

#: src/ebook-speaker.c:674 src/list_dir.c:55 src/list_dir.c:251
msgid "'h' for help"
msgstr "'h' za pomoć"

#: src/ebook-speaker.c:679
#, c-format
msgid "%d pages"
msgstr "%d strane"

#: src/ebook-speaker.c:684
#, c-format
msgid "level: %d of %d"
msgstr "nivo: %d od %d"

#: src/ebook-speaker.c:688
#, c-format
msgid "total phrases: %d"
msgstr "ukupan broj fraza: %d"

#: src/ebook-speaker.c:816
#, c-format
msgid "Be sure that the used TTS is installed:"
msgstr "Obezbedi da je korišćeni TTS instaliran:"

#: src/ebook-speaker.c:874 src/list_dir.c:165
msgid "These commands are available in this version:"
msgstr "Ove komande su dostupne u ovoj verziji:"

#: src/ebook-speaker.c:878 src/list_dir.c:169
msgid "cursor down,2   - move cursor to the next item"
msgstr "kursor dole,2  - pomeri kursor na sledeću stavku"

#: src/ebook-speaker.c:880 src/list_dir.c:171
msgid "cursor up,8     - move cursor to the previous item"
msgstr "kursor gore,8  - pomeri kursor na prethodnu stavku"

#: src/ebook-speaker.c:882
msgid "cursor right,6  - skip to next phrase"
msgstr "kursor desno,6 - skoči na sledeću frazu"

#: src/ebook-speaker.c:884
msgid "cursor left,4   - skip to previous phrase"
msgstr "kursor levo,4  - skoči na prethodnu frazu"

#: src/ebook-speaker.c:886 src/list_dir.c:177
msgid "page-down,3     - view next page"
msgstr "page-down,3    - vidi sledeću stranu"

#: src/ebook-speaker.c:888 src/list_dir.c:179
msgid "page-up,9       - view previous page"
msgstr "page-up,9      - vidi prethodnu stranu"

#: src/ebook-speaker.c:890
msgid "enter           - start playing"
msgstr "enter          - započni reprodukciju"

#: src/ebook-speaker.c:892
msgid "space,0         - pause/resume playing"
msgstr "space,0        - pauza/nastavi reprodukciju"

#: src/ebook-speaker.c:894
msgid "home,*          - play on normal speed"
msgstr "home,*         - reprodukuj u normalnoj brzini"

#: src/ebook-speaker.c:896 src/ebook-speaker.c:940
msgid "Press any key for next page..."
msgstr "Pritisni bilo koji taster za sledeću stranu..."

#: src/ebook-speaker.c:903 src/list_dir.c:183
msgid "/               - search for a label"
msgstr "/              - pretraži markere"

#: src/ebook-speaker.c:905
msgid "A               - store current item to disk in ASCII-format"
msgstr "A              - sačuvaj trenutni sadržaj na disk u ASCII-formatu"

#: src/ebook-speaker.c:907
msgid "b               - consider the end of an input line as a phrase-break"
msgstr "b              - dodeli kraj ulaznog reda prelomu fraze"

#: src/ebook-speaker.c:909
msgid "B               - move cursor to the last item"
msgstr "B              - pomeri kursor na poslednju stavku"

#: src/ebook-speaker.c:911
msgid "d               - store current item to disk in WAV-format"
msgstr "d              - sačuvaj trenutni sadržaj na disk u WAW-formatu"

#: src/ebook-speaker.c:913
msgid "D,-             - decrease playing speed"
msgstr "D,-            - smanji brzinu reprodukcije"

#: src/ebook-speaker.c:915
msgid ""
"f               - find the currently playing item and place the cursor there"
msgstr ""
"f              - pronađi trenutno reprodukovanu stavku i postavi kursor tu"

#: src/ebook-speaker.c:917
msgid "g               - go to phrase in current item"
msgstr "g              - idi na frazu u trenutnoj stavci"

#: src/ebook-speaker.c:920
msgid "G               - go to page number"
msgstr "G              - idi na stranu broj"

#: src/ebook-speaker.c:922 src/list_dir.c:187
msgid "h,?             - give this help"
msgstr "h,?            - dodeli pomoć"

#: src/ebook-speaker.c:924
msgid "j,5             - just play current item"
msgstr "j,5            - samo reprodukuj trenutnu stavku"

#: src/ebook-speaker.c:926
msgid "l               - switch to next level"
msgstr "l              - prebaci na sledeći marker"

#: src/ebook-speaker.c:928
msgid "L               - switch to previous level"
msgstr "L              - prebaci na prethodni marker"

#: src/ebook-speaker.c:930
msgid "m               - mute sound output on/off"
msgstr ""

#: src/ebook-speaker.c:932 src/list_dir.c:193
msgid "n               - search forwards"
msgstr "n              - pretraži unapred"

#: src/ebook-speaker.c:934 src/list_dir.c:195
msgid "N               - search backwards"
msgstr "N              - pretraži unazad"

#: src/ebook-speaker.c:936
msgid "o               - select next output sound device"
msgstr "o              - izaberi sledeći izlazni uređaj"

#: src/ebook-speaker.c:938
msgid "p               - place a bookmark"
msgstr "p              - postavi marker"

#: src/ebook-speaker.c:947
msgid "q               - quit eBook-speaker and place a bookmark"
msgstr "q              - zatvori eBook-speaker i postavi marker"

#: src/ebook-speaker.c:950
msgid "r               - rotate scanned document"
msgstr "r              - rotiraj skenirani dokument"

#: src/ebook-speaker.c:952
msgid "s               - stop playing"
msgstr "s              - zaustavi reprodukciju"

#: src/ebook-speaker.c:954
msgid "S               - show progress on/off"
msgstr ""

#: src/ebook-speaker.c:956
msgid "t               - select next TTS"
msgstr "t              - izaberi sledeći TTS"

#: src/ebook-speaker.c:958
msgid "T               - move cursor to the first item"
msgstr "T              - postavi kursor na prvu stavku"

#: src/ebook-speaker.c:960
msgid "U,+             - increase playing speed"
msgstr "U,+            - povećaj brzinu reprodukcije"

#: src/ebook-speaker.c:962
msgid "v,1             - decrease playback volume"
msgstr "v,1            - smanji jačinu zvuka reprodukcije"

#: src/ebook-speaker.c:964
msgid "V,7             - increase playback volume (beware of Clipping)"
msgstr ""
"V,7            - povećaj jačinu zvuka reprodukcije (pazi se Klipovanja)"

#: src/ebook-speaker.c:966
msgid "x               - go to the file-manager"
msgstr "x              - idi u fajl-menadzer"

#: src/ebook-speaker.c:968 src/list_dir.c:201 src/list_dir.c:454
msgid "Press any key to leave help..."
msgstr "Pritisni bilo koji taster da napustiš pomoć..."

#: src/ebook-speaker.c:1177 src/list_dir.c:101
msgid "What do you search?"
msgstr "Šta pretražuješ?"

#: src/ebook-speaker.c:1257
msgid "Break phrases at EOL or at a specific position (y/n/position)"
msgstr ""

#: src/ebook-speaker.c:1421
#, c-format
msgid "Language code \"%s\" is not a valid tesseract code."
msgstr "Jezički kod \"%s\" nije validan tesseract kod."

#: src/ebook-speaker.c:1423
msgid "See the tesseract manual for valid codes."
msgstr "Vidi tesseract uputstvo za validne kodove."

#: src/ebook-speaker.c:1425
#, c-format
msgid ""
"Be sure the package \"tesseract-ocr-%s\" is installed onto your system.\n"
msgstr ""
"Obezbedi da paket \"tesseract-ocr-%s\" bude instaliran na tvoj sistem.\n"

#: src/ebook-speaker.c:1434
#, c-format
msgid "Language code \"%s\" is not a valid cuneiform code."
msgstr "Jezički kod \"%s\" nije validan kod klinastog pisma."

#: src/ebook-speaker.c:1436
msgid "See the cuneiform manual for valid codes."
msgstr "Vidi uputstvo za validne kodove klinastog pisma."

#: src/ebook-speaker.c:1557
msgid "Go to phrase in current item:"
msgstr "Idi na frazu u trenutnoj stavci:"

#: src/ebook-speaker.c:1691 src/ebook-speaker.c:1930 src/ebook-speaker.c:1949
#: src/ebook-speaker.c:1974 src/ebook-speaker.c:2005 src/ebook-speaker.c:2284
#: src/ebook-speaker.c:2502 src/ebook-speaker.c:2546 src/ebook-speaker.c:2901
#: src/ebook-speaker.c:2952
msgid "eBook-speaker cannot handle this file."
msgstr "eBook-speaker ne može reprodukovati ovaj fajl."

#: src/ebook-speaker.c:1728 src/ebook-speaker.c:2420
msgid "Press 'h' for help"
msgstr "Pritisni 'h' za pomoć"

#: src/ebook-speaker.c:1983
msgid "Unknown format"
msgstr "Nepoznat format"

#: src/ebook-speaker.c:2256 src/ebook-speaker.c:2757
msgid "eBook-speaker - Version"
msgstr "eBook-speaker - Verzija"

#: src/ebook-speaker.c:2260
#, c-format
msgid ""
"Usage: %s [eBook_file | URL | -s [-r resolution]] [-b n | y | position] [-c] "
"[-d pulseaudio_device] [-h] [-H] [-i] [-o language-code] [-S] [-t "
"TTS_command] [-v]"
msgstr ""
"Iskorišćenost: %s [eBook_fajl | URL | -s [-r rezolucija]] [-o jezički-kod] [-"
"d pulseaudio_zvučni_uređaj] [-h] [-H] [-i] [-S] [-t TTS_komanda] [-b n | y | "
"position] [-c] [-v]"

#: src/list_dir.c:51 src/list_dir.c:248
#, c-format
msgid "%s - Choose an input-file"
msgstr "%s - Izaberi ulazni-fajl"

#: src/list_dir.c:173
msgid "cursor right,6  - open this directory or file"
msgstr "krsor desno,6  - otvori ovaj direktorijum ili fajl"

#: src/list_dir.c:175
msgid "cursor left,4   - open previous directory"
msgstr "kursor levo,4  - otvori prethodni direktorijum"

#: src/list_dir.c:181
msgid "enter,6         - open this directory or file"
msgstr "enter,6        - otvori ovaj direktorijum ili fajl"

#: src/list_dir.c:185
msgid "end,B           - move cursor to the last item"
msgstr "end,B          - pomeri kursor na poslednju stavku"

#: src/list_dir.c:189
msgid "H,0             - display \"hidden\" files on/off"
msgstr "H,0            - prikaži \"skrivene\" fajlove uključeno/isključeno"

#: src/list_dir.c:191
msgid "i               - give some information about current file"
msgstr ""

#: src/list_dir.c:197
msgid "q               - quit eBook-speaker"
msgstr "q              - izađi iz programa eBook-speaker"

#: src/list_dir.c:199
msgid "home,T          - move cursor to the first item"
msgstr "home,T         - pomeri kursor na prvu stavku"

