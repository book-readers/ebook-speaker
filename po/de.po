#, c-format
msgid "%s - Choose an input-file"
msgstr "%s - Wählen Sie eine Input-Datei"

#: src/list_dir.c:188
msgid "cursor right,6  - open this directory or file"
msgstr "Pfeil rechts,6 - öffnen Sie dieses Verzeichnis oder Datei"

#: src/list_dir.c:190
msgid "cursor left,4   - open previous directory"
msgstr "Pfeil links,4  - öffnen vorherige Verzeichnis"

#: src/list_dir.c:196
msgid "enter,6         - open this directory or file"
msgstr "Eingabetaste,6 - öffnen Sie dieses Verzeichnis oder Datei"

#: src/list_dir.c:200
msgid "end,B           - move cursor to the last item"
msgstr "end,B          - Cursor auf das letzte Element"

#: src/list_dir.c:204
msgid "H,0             - display \"hidden\" files on/off"
msgstr "H,0            - Anzeige \"hidden\" Dateien on/off"

#: src/list_dir.c:210
msgid "q               - quit eBook-speaker"
msgstr "q              - eBook-speaker verlassen"

#: src/list_dir.c:212
msgid "home,T          - move cursor to the first item"
msgstr "home,T         - Cursor auf das erste Element"

