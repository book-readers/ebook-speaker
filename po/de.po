#: src/ebook-speaker.c:3146
msgid "eBook-speaker cannot handle this file."
msgstr "eBook-speaker kann diese Datei nicht verarbeiten"

#: src/ebook-speaker.c:1726 src/ebook-speaker.c:2584
msgid "Press 'h' for help"
msgstr "'h' für Hilfe drücken"

#: src/ebook-speaker.c:2002
msgid "Unknown format"
msgstr "Unbekanntest Format"

#: src/ebook-speaker.c:2417 src/ebook-speaker.c:2929
msgid "eBook-speaker - Version"
msgstr "eBook-speaker - Version"

#: src/ebook-speaker.c:2421
#, c-format
msgid ""
"Usage: %s [eBook_file | URL | -s [-r resolution]] [-b n | y | position] [-c] "
"[-d audio_device:audio_type] [-h] [-H] [-i] [-o language-code] [-S] [-t "
"TTS_command] [-v]"
msgstr ""
"Benutzung : %s [eBook_Datei | URL | -s [-r Auflösung]] [-b n | y | Position] "
"[-c] [-d audio_device:audio_type] [-h] [-H] [-i] [-o Sprachcode] [-S] [-t "
"TTS_Befehl] [-v]"

#: src/list_dir.c:51 src/list_dir.c:248
#, c-format
msgid "%s - Choose an input-file"
msgstr "%s - Wählen Sie eine zum Öffnen"

#: src/list_dir.c:173
msgid "cursor right,6  - open this directory or file"
msgstr "Pfeil rechts,6 - öffnen Sie dieses Verzeichnis oder Datei"

#: src/list_dir.c:175
msgid "cursor left,4   - open previous directory"
msgstr "Pfeil links,4  - öffnen vorherige Verzeichnis"

#: src/list_dir.c:181
msgid "enter,6         - open this directory or file"
msgstr "Eingabetaste,6 - öffnen Sie dieses Verzeichnis oder Datei"

#: src/list_dir.c:185
msgid "end,B           - move cursor to the last item"
msgstr "end,B          - Cursor auf das letzte Element"

#: src/list_dir.c:189
msgid "H,0             - display \"hidden\" files on/off"
msgstr "H,0            - Anzeige \"versteckte\" Dateien ein/aus"

#: src/list_dir.c:191
msgid "i               - give some information about current file"
msgstr "i              - Zeige Informationen zur aktuellen Datei"

#: src/list_dir.c:197
msgid "q               - quit eBook-speaker"
msgstr "q              - eBook-speaker verlassen"

#: src/list_dir.c:199
msgid "home,T          - move cursor to the first item"
msgstr "home,T         - Cursor auf das erste Element"

