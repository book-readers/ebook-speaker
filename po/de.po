msgid "h,?             - give this help"
msgstr "h,?            - diese Hilfe anzeigen"

#: src/ebook-speaker.c:1231
msgid "j,5             - just play current item"
msgstr "j,5            - nur das aktuelle Element abspielen"

#: src/ebook-speaker.c:1233
msgid "l               - switch to next level"
msgstr "l              - zum nächsten Level wechseln"

#: src/ebook-speaker.c:1235
msgid "L               - switch to previous level"
msgstr "L              - zum vorigen Level wechseln"

#: src/ebook-speaker.c:1237
msgid "m               - mute sound output on/off"
msgstr ""

#: src/ebook-speaker.c:1239 src/list_dir.c:193
msgid "n               - search forwards"
msgstr "n              - Suche vorwärts"

#: src/ebook-speaker.c:1241 src/list_dir.c:195
msgid "N               - search backwards"
msgstr "N              - Suche rückwärts"

#: src/ebook-speaker.c:1243
msgid "o               - select next output sound device"
msgstr "o              - nächstes sound device für Ausgabe wählen"

#: src/ebook-speaker.c:1245
msgid "p               - place a bookmark"
msgstr "p              - Lesezeichen setzen"

#: src/ebook-speaker.c:1254
msgid "q               - quit eBook-speaker and place a bookmark"
msgstr "q              - eBook-speaker verlassen und Lesezeichen setzen"

#: src/ebook-speaker.c:1257
msgid "r               - rotate scanned document"
msgstr ""

#: src/ebook-speaker.c:1259
msgid "s               - stop playing"
msgstr "s              - Abspielen stoppen"

#: src/ebook-speaker.c:1261
msgid "t               - select next TTS"
msgstr "t              - Wählen nächsten TTS"

#: src/ebook-speaker.c:1263
msgid "T               - move cursor to the first item"
msgstr "T              - Cursor auf das erste Element"

#: src/ebook-speaker.c:1265
msgid "U,+             - increase playing speed"
msgstr "U,+            - erhöhe Abspielgeschwindigkeit"

#: src/ebook-speaker.c:1267
msgid "v,1             - decrease playback volume"
msgstr ""

#: src/ebook-speaker.c:1269
msgid "V,7             - increase playback volume (beware of Clipping)"
msgstr ""

